<div class="pir-results-container">
    {{#if items}}
    <section class="pir-results-body">
        <div class="pir-table">
            <div class="pir-table-header">
                <div class="pir-table-cell">Actor</div>
                <div class="pir-table-cell">Type</div>
                <div class="pir-table-cell">Actions</div>
                <div class="pir-table-cell">Changed Property</div>
                <div class="pir-table-cell">Actor Item Link</div>
                <div class="pir-table-cell">Origin Item Link</div>
            </div>

        {{#each items}}
            <div class="pir-table-row">
                <div class="pir-table-cell">{{{this.actorLink}}}</div>
                <div class="pir-table-cell">{{this.type}}</div>
                <div class="pir-table-cell pir-button-holder">
                    {{#each this.buttons}}
                        {{#unless this.hidden}} 
                            <button class="pir-button" {{#if this.disabled}} disabled="true" {{/if}} title={{#if this.disabled}} "{{this.disabled}}" {{^}} "{{this.title}}" {{/if}} data-action="{{this.action}}" value="{{this.value}}">
                                <i class="{{this.icon}}"></i>
                            </button>
                        {{/unless}}
                    {{/each}}
                </div>
                <div class="pir-table-cell">{{{this.comparativeDataText}}}</div>
                <div class="pir-table-cell">{{{this.actorItemLink}}}</div>
                <div class="pir-table-cell">{{{this.originItemLink}}}</div>
            </div>
        {{/each}}
        </div>
    </section>
    {{else}}
    <h2>âœ… No changed items found</h2><p>Searched through the following actors:<br>{{this.actorIds}}</p>
    {{/if}}
</div>